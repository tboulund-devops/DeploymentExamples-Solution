pipeline {
    agent any
    parameters {
        run description: '', filter: 'SUCCESSFUL', name: 'PROMOTE_BUILD', projectName: 'Teacher/CD/Deployment'
    }
    stages {
        stage("Release to production") {
            steps {
                sh "docker-compose -p production -f docker-compose.yml -f docker-compose.prod.yml up -d"
            }
        }
    }
}